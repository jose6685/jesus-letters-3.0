{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/app.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json",
      "headers": {
        "Content-Type": "application/json; charset=utf-8"
      }
    },
    {
      "src": "/favicon.svg",
      "dest": "/favicon.svg",
      "headers": {
        "Content-Type": "image/svg+xml"
      }
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.svg",
      "headers": {
        "Content-Type": "image/svg+xml"
      }
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}